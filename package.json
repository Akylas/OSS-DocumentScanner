{
    "nativescript": {
        "id": "com.akylas.documentscanner",
        "tns-android": {
            "version": "6.5.3"
        },
        "tns-ios": {
            "version": "7.0.0-beta.3-v8-2020-07-01-222820-05"
        }
    },
    "scripts": {
        "run.android.production": "cross-var tns run android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.production --env.uglify --env.adhoc  --env.snapshot --env.compileSnapshot",
        "run.ios.production": "tns run ios --release --env.adhoc --env.snapshot --env.compileSnapshot --for-device --provision 'match AppStore com.akylas.documentscanner'",
        "pandroid": "cross-var tns build android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.snapshot --env.compileSnapshot",
        "pios": "tns run ios --release --env.adhoc --env.snapshot --env.compileSnapshot --for-device --provision 'match AppStore com.akylas.documentscanner'",
        "ios.generate_licences": "node generate_ios_licences.js",
        "android.generate_licences": "cd platforms/android && ./gradlew  generateLicenseReport"
    },
