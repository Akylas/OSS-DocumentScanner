{
    "whitelist-plugins-usages": true,
    "whitelist": [
        "com.google.android.material.color:DynamicColors",
        "androidx.core.splashscreen:SplashScreen",
        "com.akylas.documentscanner:Utils",
        "com.akylas.documentscanner:Utils.Companion",
        "com.akylas.documentscanner:PDFUtils",
        "com.akylas.documentscanner:PDFUtils.Companion",
        "com.akylas.documentscanner.utils:ImageUtil",
        "com.akylas.documentscanner.utils:ImageUtil.Companion",
        "com.akylas.documentscanner:PDFUtils.FunctionCallback",
        "androidx.appcompat.app:AppCompatDelegate",
        "androidx.core.os:LocaleListCompat",
        "java.nio.file:Files",
        "java.io:FileOutputStream",
        "androidx.core.widget:NestedScrollView",
        "android.text.format:DateFormat",
        "android.graphics:BitmapShader",
        "androidx.documentfile.provider:DocumentFile",
        "android.widget:Toast"
    ],
    "blacklist": [
        "android.widget:DatePicker",
        "android.widget:DatePicker.OnDateChangedListener",
        "android.widget:NumberPicker",
        "android.widget:NumberPicker.Formatter",
        "android.widget:NumberPicker.OnValueChangeListener",
        "android.widget:TabHost",
        "android.widget:TabHost.OnTabChangeListener",
        "android.widget:TabHost.TabContentFactory",
        "android.widget:TabHost.TabSpec",
        "android.widget:TabWidget",
        "android.widget:TimePicker",
        "android.widget:TimePicker.OnTimeChangedListener",
        "androidx.appcompat.widget:SearchView",
        "androidx.appcompat.widget:SearchView.OnCloseListener",
        "androidx.appcompat.widget:SearchView.OnQueryTextListener",
        "androidx.appcompat.widget:Toolbar.OnMenuItemClickListener",
        "androidx.appcompat.widget:AppCompatTextView",

        "org.ow2.asmdex*:*",
        "org.nativescript.widgets:TabItemSpec*",
        "org.nativescript.widgets:TabsBar*",
        "org.nativescript.widgets:SegmentedBar*",
        "org.nativescript.widgets:TabViewPager*",
        "org.nativescript.widgets:TabLayout*",
        "org.nativescript.widgets:BottomNavigationBar*",
        "com.google.android.material.chip*:*",
        "com.google.android.material.appbar*:*",
        "com.google.android.material.floatingactionbutton*:*",
        "com.google.android.material.bottomappbar*:*",
        "com.google.android.material.internal*:*",
        "com.google.android.material.transformation*:*",
        "com.google.android.material.theme*:*",
        "*:*BuildConfig",
        "*:R.*",
        "*:R",
        "*:R$"
    ],
    "proguard_blacklist": [
        "org.nativescript.widgets:TabItemSpec*",
        "org.nativescript.widgets:TabsBar*",
        "org.nativescript.widgets:SegmentedBar*",
        "org.nativescript.widgets:TabViewPager*",
        "org.nativescript.widgets:TabLayout*",
        "org.nativescript.widgets:BottomNavigationBar*",
        "com.google.android.material.chip*:*",
        "com.google.android.material.appbar*:*",
        "com.google.android.material.floatingactionbutton*:*",
        "com.google.android.material.bottomappbar*:*",
        "com.google.android.material.internal*:*",
        "com.google.android.material.transformation*:*",
        "com.google.android.material.theme*:*"
    ]
}