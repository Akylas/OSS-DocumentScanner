<svg xmlns="http://www.w3.org/2000/svg" width="300" height="600" viewBox="0 0 300 600">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2e8555;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#205d3b;stop-opacity:1" />
    </linearGradient>
  </defs>
  <!-- Background -->
  <rect width="300" height="600" fill="url(#bg)"/>
  <!-- Phone frame -->
  <rect x="20" y="20" width="260" height="560" rx="20" fill="#1a1a1a"/>
  <!-- Screen -->
  <rect x="30" y="50" width="240" height="500" fill="#f5f5f5"/>
  <!-- Export options header -->
  <rect x="30" y="50" width="240" height="30" fill="#fff"/>
  <text x="150" y="70" text-anchor="middle" font-family="Arial" font-size="12" fill="#333">Export Options</text>
  <!-- PDF option -->
  <rect x="45" y="100" width="210" height="60" rx="8" fill="#fff" stroke="#ddd"/>
  <rect x="55" y="110" width="40" height="40" rx="4" fill="#e74c3c"/>
  <text x="75" y="135" text-anchor="middle" font-family="Arial" font-size="10" fill="#fff">PDF</text>
  <text x="140" y="125" font-family="Arial" font-size="12" fill="#333">PDF Document</text>
  <text x="140" y="145" font-family="Arial" font-size="10" fill="#999">With OCR text layer</text>
  <!-- JPEG option -->
  <rect x="45" y="175" width="210" height="60" rx="8" fill="#fff" stroke="#ddd"/>
  <rect x="55" y="185" width="40" height="40" rx="4" fill="#3498db"/>
  <text x="75" y="210" text-anchor="middle" font-family="Arial" font-size="10" fill="#fff">JPG</text>
  <text x="140" y="200" font-family="Arial" font-size="12" fill="#333">JPEG Image</text>
  <text x="140" y="220" font-family="Arial" font-size="10" fill="#999">Compressed format</text>
  <!-- PNG option -->
  <rect x="45" y="250" width="210" height="60" rx="8" fill="#fff" stroke="#25c2a0" stroke-width="2"/>
  <rect x="55" y="260" width="40" height="40" rx="4" fill="#25c2a0"/>
  <text x="75" y="285" text-anchor="middle" font-family="Arial" font-size="10" fill="#fff">PNG</text>
  <text x="140" y="275" font-family="Arial" font-size="12" fill="#333">PNG Image</text>
  <text x="140" y="295" font-family="Arial" font-size="10" fill="#999">Lossless quality</text>
  <!-- Share option -->
  <rect x="45" y="325" width="210" height="60" rx="8" fill="#fff" stroke="#ddd"/>
  <circle cx="75" cy="355" r="20" fill="#9b59b6"/>
  <text x="75" y="360" text-anchor="middle" font-family="Arial" font-size="14" fill="#fff">â†—</text>
  <text x="140" y="350" font-family="Arial" font-size="12" fill="#333">Share</text>
  <text x="140" y="370" font-family="Arial" font-size="10" fill="#999">Send to other apps</text>
  <!-- Export button -->
  <rect x="60" y="420" width="180" height="50" rx="25" fill="#25c2a0"/>
  <text x="150" y="450" text-anchor="middle" font-family="Arial" font-size="14" fill="#fff">Export</text>
  <!-- Placeholder text -->
  <text x="150" y="550" text-anchor="middle" font-family="Arial" font-size="10" fill="#999">Placeholder - Replace with Maestro screenshot</text>
</svg>
