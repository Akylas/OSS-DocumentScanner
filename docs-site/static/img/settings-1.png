<svg xmlns="http://www.w3.org/2000/svg" width="300" height="600" viewBox="0 0 300 600">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2e8555;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#205d3b;stop-opacity:1" />
    </linearGradient>
  </defs>
  <!-- Background -->
  <rect width="300" height="600" fill="url(#bg)"/>
  <!-- Phone frame -->
  <rect x="20" y="20" width="260" height="560" rx="20" fill="#1a1a1a"/>
  <!-- Screen -->
  <rect x="30" y="50" width="240" height="500" fill="#f5f5f5"/>
  <!-- Header -->
  <rect x="30" y="50" width="240" height="30" fill="#fff"/>
  <text x="150" y="70" text-anchor="middle" font-family="Arial" font-size="12" fill="#333">Settings</text>
  <!-- General section -->
  <text x="45" y="105" font-family="Arial" font-size="10" fill="#666">GENERAL</text>
  <!-- Language -->
  <rect x="45" y="115" width="210" height="45" fill="#fff"/>
  <text x="55" y="142" font-family="Arial" font-size="11" fill="#333">Language</text>
  <text x="235" y="142" text-anchor="end" font-family="Arial" font-size="11" fill="#999">English ›</text>
  <!-- Theme -->
  <rect x="45" y="162" width="210" height="45" fill="#fff"/>
  <text x="55" y="189" font-family="Arial" font-size="11" fill="#333">Theme</text>
  <text x="235" y="189" text-anchor="end" font-family="Arial" font-size="11" fill="#999">System ›</text>
  <!-- Camera section -->
  <text x="45" y="235" font-family="Arial" font-size="10" fill="#666">CAMERA</text>
  <!-- Quality -->
  <rect x="45" y="245" width="210" height="45" fill="#fff"/>
  <text x="55" y="272" font-family="Arial" font-size="11" fill="#333">Capture Quality</text>
  <text x="235" y="272" text-anchor="end" font-family="Arial" font-size="11" fill="#999">High ›</text>
  <!-- Auto capture -->
  <rect x="45" y="292" width="210" height="45" fill="#fff"/>
  <text x="55" y="319" font-family="Arial" font-size="11" fill="#333">Auto Capture</text>
  <!-- Toggle switch ON -->
  <rect x="200" y="305" width="40" height="22" rx="11" fill="#25c2a0"/>
  <circle cx="228" cy="316" r="9" fill="#fff"/>
  <!-- Export section -->
  <text x="45" y="365" font-family="Arial" font-size="10" fill="#666">EXPORT</text>
  <!-- Default format -->
  <rect x="45" y="375" width="210" height="45" fill="#fff"/>
  <text x="55" y="402" font-family="Arial" font-size="11" fill="#333">Default Format</text>
  <text x="235" y="402" text-anchor="end" font-family="Arial" font-size="11" fill="#999">PDF ›</text>
  <!-- OCR -->
  <rect x="45" y="422" width="210" height="45" fill="#fff"/>
  <text x="55" y="449" font-family="Arial" font-size="11" fill="#333">Include OCR</text>
  <!-- Toggle switch ON -->
  <rect x="200" y="435" width="40" height="22" rx="11" fill="#25c2a0"/>
  <circle cx="228" cy="446" r="9" fill="#fff"/>
  <!-- Placeholder text -->
  <text x="150" y="550" text-anchor="middle" font-family="Arial" font-size="10" fill="#999">Placeholder - Replace with Maestro screenshot</text>
</svg>
